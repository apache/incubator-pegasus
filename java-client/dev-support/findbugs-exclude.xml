            <Package name="org.apache.pegasus.client"/>
            <Package name="org.apache.pegasus.metrics"/>
            <Package name="org.apache.pegasus.tools"/>
            <Package name="org.apache.pegasus.client.request"/>
            <Package name="org.apache.pegasus.example"/>
        </Or>
    </Match>

</FindBugsFilter>
